{
  "name": "krehak/laravel-live-edit",
  "description": "Edit your texts simple inside a running Laravel installation without the need to make custom administration",
  "keywords": [
    "laravel", "package", "live edit", "editor", "no admin"
  ],
  "version": "1.0.2",
  "homepage": "https://github.com/krehak/laravel-live-edit",
  "license": "MIT",
  "authors": [
    {
      "name": "Patrik Kreh√°k",
      "email": "contact@ptk.sk",
      "homepage": "https://krehak.com/"
    }
  ],
  "autoload": {
    "psr-4": {
      "Krehak\\LiveEdit\\": "LiveEdit"
    }
  },
  "extra": {
    "laravel": {
      "providers": [
        "Krehak\\LiveEdit\\LiveEditServiceProvider"
      ]
    },
    "branch-alias": {
      "dev-master": "1.0-dev"
    }
  },
  "prefer-stable": true,
  "minimum-stability": "dev",
  "require": {
    "php": "^8.0",
    "laravel/framework": "^8"
  }
}
